{
  "version": 2,
  "builds": [
    {
      "src": "learning_log/wsgi.py",
      "use": "@vercel/python",
      "config":{ "maxlambdaSize": "15mb","runtime":"python3.13"}

    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"

    },
    {
        "src": "/(.*)",
        "dest": "learning_log/wsgi.py"

    }
  ]
}
